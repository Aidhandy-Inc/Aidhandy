{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/cookie-policy", "destination": "/cookie-policy.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/messaging-terms", "destination": "/messaging-terms.html" },
    { "source": "/thankyou", "destination": "/thankyou.html" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/favicon-96x96.png", "destination": "/favicon-96x96.png" },
    { "source": "/favicon.svg", "destination": "/favicon.svg" },
    { "source": "/apple-touch-icon.png", "destination": "/apple-touch-icon.png" },
    { "source": "/site.webmanifest", "destination": "/site.webmanifest" },

    { "source": "/(.*)", "destination": "/nextapp/$1" }
  ]
}
