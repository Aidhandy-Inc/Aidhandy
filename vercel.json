{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/:file(.*)", "destination": "/:file" },

    { "source": "/app/:path*", "destination": "/nextapp/app/:path*" },
    { "source": "/auth/:path*", "destination": "/nextapp/app/auth/:path*" },
    { "source": "/dashboard/:path*", "destination": "/nextapp/app/dashboard/:path*" },
    { "source": "/verify-email/:path*", "destination": "/nextapp/app/verify-email/:path*" },
    { "source": "/stripe/:path*", "destination": "/nextapp/app/stripe/:path*" }
  ]
}
